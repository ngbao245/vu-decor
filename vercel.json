{
  "redirects": [
    {
      "source": "/snotes",
      "destination": "/snotes/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/snotes/",
      "destination": "https://note-silk-gamma.vercel.app/"
    },
    {
      "source": "/snotes/:path+",
      "destination": "https://note-silk-gamma.vercel.app/:path+"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
